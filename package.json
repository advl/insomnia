{
  "name": "template-react-ssr",
  "version": "2.3.0",
  "description": "",
  "main": "public/index.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client:dev:start": "NODE_ENV=development webpack-dev-server --progress --config ./webpack.client.dev.js --watch",
    "client:dev:compile": "NODE_ENV=development BACKEND=false webpack --config ./webpack.client.dev.js",
    "client:prod:start": "NODE_ENV=production webpack-dev-server --progress --config ./webpack.client.prod.js --watch",
    "client:prod:compile": "NODE_ENV=production COMPILE_FOR_SSR=true COMPILE=true BACKEND=false webpack --config ./webpack.client.prod.js",
    "cds": "npm run client:dev:start",
    "cdc": "npm run client:dev:compile",
    "cdcw": "npm run cdc -- -- --watch",
    "cps": "npm run client:prod:start",
    "cpc": "npm run client:prod:compile",
    "cpcw": "npm run cpc -- -- --watch",
    "renderer:start": "NODE_ENV=development nodemon src/ssr/server.js",
    "renderer:compile": "NODE_ENV=production COMPILE=true webpack --config ./webpack.renderer.js",
    "sitemap:start": "NODE_ENV=development nodemon src/sitemap/server.js",
    "sitemap:compile": "NODE_ENV=production COMPILE=true webpack --config ./webpack.sitemap.js",
    "rs": "BACKEND=true npm run renderer:start",
    "rc": "BACKEND=true npm run renderer:compile",
    "rt": "BACKEND=true npm run renderer:test",
    "ss": "BACKEND=true npm run sitemap:start",
    "sc": "BACKEND=true npm run sitemap:compile",
    "build:local": "NODE_ENV=production COMPILE_FOR_SSR=true npm run cpc && npm run rc && npm run sc",
    "old:now-build": "npm run cpc",
    "deploy-commit": "git add . && git commit -m '[Auto] Compile and deploy from cli' && git push",
    "build:deploy": "npm run build:local && npm run deploy-commit",
    "bd": "npm run build:deploy",
    "test-eslint": "eslint src/client.js",
    "relink": "npm link @fwrlines/ds @fwrlines/ds.core @fwrlines/ds.contrib @fwrlines/ds.form @fwrlines/stylebook @fwrlines/swatch @fwrlines/utils @fwrlines/destination @fwrlines/assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fwrlines/template-react-ssr.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fwrlines/template-react-ssr/issues"
  },
  "homepage": "https://github.com/fwrlines/template-react-ssr#readme",
  "dependencies": {
    "@apollo/client": "^3.0.0-rc.2",
    "@apollo/link-retry": "^2.0.0-beta.3",
    "@apollo/react-ssr": "^4.0.0-beta.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@fwrlines/destination": "latest",
    "@fwrlines/ds": "latest",
    "@fwrlines/stylebook": "latest",
    "@fwrlines/swatch": "latest",
    "@fwrlines/utils": "latest",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "date-fns": "^2.14.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "graphql": "^15.1.0",
    "graphql-tag": "^2.10.3",
    "mdx.macro": "^0.2.9",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.1.2",
    "react-helmet": "^6.1.0",
    "react-intl": "^4.6.9",
    "react-router-dom": "^5.2.0",
    "react-scroll": "^1.8.1",
    "save": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@mdx-js/loader": "^1.6.22",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-intl": "^7.5.20",
    "babel-plugin-react-intl-extractor": "^1.0.8",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.5.3",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.2.0",
    "eslint-config-synacor": "^3.0.5",
    "eslint-plugin-react-hooks": "^4.0.4",
    "html-webpack-plugin": "^4.3.0",
    "ignore-loader": "^0.1.2",
    "jsonlint": "^1.6.3",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "postcss": "^8.2.10",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "react-loadable-ssr-addon": "^0.2.2",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^1.7.2"
  }
}
